{
    "[php]": {
        "editor.defaultFormatter": "kokororin.vscode-phpfmt"
    },
    "ansible.azureRestSpec": "/home/jharry/.vscode/rest",
    "ansible.credentialsConfigured": true,
    "breadcrumbs.enabled": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorWidth": 5,
    "editor.fontFamily": "'Fira Code Regular','Hack', 'monospace', monospace",
    "editor.fontSize": 15,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80],
    "editor.tabSize": 2,
    "explorer.autoReveal": false,
    "git.autofetch": true,
    "shellcheck.disableVersionCheck": true,
    "telemetry.enableTelemetry": false,
    "terminal.external.linuxExec": "/usr/bin/tilix",
    "terminal.integrated.fontFamily": "'Hack Nerd Font'",
    "terminal.integrated.fontSize": 15,
    "window.title": "${activeEditorLong}",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Dracula Soft",
    "workbench.colorCustomizations": {
        // "activityBar.background": "#021e45",
        // "statusBar.background": "#021e45",
        // "statusBar.foreground": "#e0e0e0",
        // "editorLineNumber.foreground": "#d1d1d1",
        // "editorOverviewRuler.errorForeground": "#fff"
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/_site": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.pytest_cache": true,
        "**/.svn": true,
        "**/*.asset-cache": true,
        "**/*.cache": true,
        "**/*.coverage": true,
        "**/*.egg-info": true,
        "**/*.jekyll-cache": true,
        "**/*.pyc": true,
        "**/*.tweet-cache": true,
        "**/bower_components": true,
        "**/celerybeat-schedule": true,
        "**/CVS": true,
        "**/node_modules": true,
        "**/tmp": true,
    },
    "[yaml]": {
        "editor.autoIndent": false
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    
}
